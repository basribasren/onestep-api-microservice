# EditorConfig is awesome: http://EditorConfig.org

# top-most EditorConfig file
root = true

# Windows-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charse
charset = utf-8

# 4 tab indentation
indent_style = tab
indent_size = 4

# trim_trailing_whitespace = true
